{
  "name": "setup-antlr4",
  "version": "0.0.6",
  "private": true,
  "description": "Setup ANTLR 4 action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "bundle": "ncc build",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run bundle && yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiccoMlt/setup-antlr4.git"
  },
  "keywords": [
    "actions",
    "setup",
    "antlr",
    "antlr4"
  ],
  "author": {
    "name": "Niccol√≤ Maltoni",
    "email": "nicco.mlt@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/tool-cache": "^1.3.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.3.1",
    "eslint-plugin-github": "^4.0.1",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.6.4"
  }
}
