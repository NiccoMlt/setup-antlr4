{
  "name": "setup-antlr4",
  "version": "0.0.1",
  "private": true,
  "description": "Setup ANTLR 4 action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "bundle": "ncc build",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run bundle && yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiccoMlt/setup-antlr4.git"
  },
  "keywords": [
    "actions",
    "setup",
    "antlr"
  ],
  "author": {
    "name": "Niccol√≤ Maltoni",
    "email": "nicco.mlt@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/tool-cache": "^1.3.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
